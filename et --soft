[32m+[m[32m}}[m
[32m+[m[32m"""[m
[32m+[m
[32m+[m[32m    response = client.chat.completions.create([m
[32m+[m[32m        model="gpt-4o-mini",[m
[32m+[m[32m        messages=[[m
[32m+[m[32m            {"role": "system", "content": "ë„ˆëŠ” CX ë¶„ì„ ì „ë¬¸ê°€ë‹¤."},[m
[32m+[m[32m            {"role": "user", "content": prompt}[m
[32m+[m[32m        ],[m
[32m+[m[32m        temperature=0.3[m
[32m+[m[32m    )[m
[32m+[m
[32m+[m[32m    content = response.choices[0].message.content[m
[32m+[m[32m    match = re.search(r"\{.*\}", content, re.DOTALL)[m
[32m+[m
[32m+[m[32m    return json.loads(match.group())[m
